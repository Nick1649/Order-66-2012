package {		import flash.display.MovieClip;	import flash.events.*;    import flash.events.KeyboardEvent;	import flash.events.MouseEvent;	import flash.text.TextField;  // used for scores	import flash.text.TextFormat  // used to set color to text	import flash.media.Sound;	import flash.display.DisplayObject;	import fl.motion.MotionEvent;	import flash.net.URLLoader;	import flash.net.URLRequest;	import flash.utils.Timer;			public class laserClass extends MovieClip	{		private var speed:int;				public function laserClass()  // constructor		{			speed = 100;			trace ("laser created");			this.addEventListener(Event.ENTER_FRAME, update);		}				public function update (event:Event) : void		{			//trace ("Laser at" + this.x + " and " + this.y);			x += Math.cos(rotation/180*Math.PI)*speed;			y += Math.sin(rotation/180*Math.PI)*speed;					}	}}